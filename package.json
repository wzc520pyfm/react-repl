{
  "name": "antd-playground",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "description": "Antd Playground",
  "type": "module",
  "keywords": [
    "antd",
    "ant-design",
    "playground",
    "react",
    "sandpack"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache --max-warnings 0 .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "format": "prettier --write --cache ."
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.19.13",
    "@unocss/reset": "^65.5.0",
    "antd": "^5.24.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@iconify-json/ri": "^1.2.5",
    "@sxzz/eslint-config": "^5.3.0",
    "@sxzz/prettier-config": "^2.2.3",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/semver": "^7.7.0",
    "@unocss/eslint-plugin": "^65.5.0",
    "@unocss/transformer-directives": "^65.5.0",
    "@vitejs/plugin-react": "^4.3.4",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "unocss": "^65.5.0",
    "vite": "^6.3.5",
    "vite-plugin-inspect": "^10.3.0",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "engines": {
    "node": ">=20.10"
  },
  "prettier": "@sxzz/prettier-config"
}
